numero = int(input("Digite um número: "))

if numero > 0:
    if numero %2 ==0:
        print("O número", numero,"é par e positivo")
    else:
        print("O número", numero,"é ímpar e positivo")

elif numero < 0:
    print("O número", numero,"é negativo")
else:
    print("O número", numero,"é zero")